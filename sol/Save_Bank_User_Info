pragma solidity ^0.4.18;
contract SaveUserInfo{
    string private username;
    string private userID;
    string private userbirthday;
    string private userphone;
    string private useraddress;
    string private sealfilename;
    string private sealfileformat;
    string private sealipfshash;
    string private savedatetime;
    address private bankaddress;
    
    
    function saveUserInfo(address bankaddr,string usname,string usid,string usbirday,string usphone,string usaddress,string sealfname,string sealfformat,string sealhash,string savedt)public{
       bankaddress=bankaddr;
       username=usname;
       userID=usid;
       userbirthday=usbirday;
       userphone=usphone;
       useraddress=usaddress;
       sealfilename=sealfname;
       sealfileformat=sealfformat;
       sealipfshash=sealhash;
       savedatetime=savedt;
    }
}